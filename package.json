{
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-toastify.umd.cjs",
  "module": "./dist/vue-toastify.js",
  "exports": {
    ".": {
      "import": "./dist/vue-toastify.js",
      "require": "./dist/vue-toastify.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "types": "./dist/types/index.d.ts",
  "name": "vue-toastify-3",
  "version": "1.0.9",
  "private": false,
  "author": "Ihor Ivliev",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run generate:types",
    "preview": "vite preview",
    "build-only": "vite build",
    "generate:types": "vue-tsc --declaration --emitDeclarationOnly --outdir ./dist",
    "type-check": "vue-tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/holdenn1/vue-toastify.git"
  },
  "dependencies": {
    "sass": "^1.62.0",
    "vue": "^3.2.47"
  },
  "keywords": [
    "vue",
    "toast",
    "toastify",
    "typescript",
    "ts"
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/tsconfig": "^0.1.3",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vue-tsc": "^1.6.4"
  }
}
