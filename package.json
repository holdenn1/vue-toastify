{
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-toastify.umd.cjs",
  "module": "./dist/vue-toastify.js",
  "exports": {
    ".": {
      "import": "./dist/vue-toastify.js",
      "require": "./dist/vue-toastify.umd.cjs"
    }
  },
  "name": "vue-toastify",
  "version": "1.0.0",
  "private": false,
  "types": "./dist/types.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run generate:types",
    "preview": "vite preview",
    "build-only": "vite build",
    "generate:types": "vue-tsc --declaration --emitDeclarationOnly --outdir ./dist",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "sass": "^1.62.0",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/tsconfig": "^0.1.3",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vue-tsc": "^1.6.4"
  }
}
